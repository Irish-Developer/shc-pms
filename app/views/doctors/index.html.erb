<!--This html code is used to display the Doctors page-->
<% provide(:title, 'All doctors') %>
<h1>All Doctors</h1>

<ul class="list_css">
  <!--Get doctors-->
  <% @doctors.each do |doctor| %>
    <li>
      <!--Display doctors image and name-->
      <%= gravatar_for doctor, size: 50 %>
      <%= link_to doctor.fname, doctor %>
    </li>
  <% end %>
</ul